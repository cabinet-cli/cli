{
    "name": "cabinet-cli",
    "description": "A normal web archiver but aims to work for literally everything\n",
    "version": "0.0.1",
    "author": "Sophia <async3619@naver.com>",
    "bin": {
        "cabinet": "./bin/run"
    },
    "bugs": "https://github.com/cabinet-cli/core/issues",
    "dependencies": {
        "@oclif/command": "^1",
        "@oclif/config": "^1",
        "@oclif/plugin-help": "^3",
        "fs-extra": "^9.0.1",
        "lodash": "^4.17.20",
        "mime-types": "^2.1.27",
        "tslib": "^2.0.3",
        "yaml": "^1.10.0",
        "yup": "^0.29.3"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1",
        "@oclif/test": "^1",
        "@types/chai": "^4.2.14",
        "@types/fs-extra": "^9.0.4",
        "@types/lodash": "^4.14.165",
        "@types/mime-types": "^2.1.0",
        "@types/mocha": "^8.2.0",
        "@types/node": "^14.14.11",
        "@types/yup": "^0.29.10",
        "@typescript-eslint/eslint-plugin": "^4.9.1",
        "@typescript-eslint/parser": "^4.9.1",
        "chai": "^4.2.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.15.0",
        "eslint-config-prettier": "^7.0.0",
        "globby": "^10",
        "mocha": "^8.2.1",
        "nyc": "^15.1.0",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-node": "^9.1.1",
        "typescript": "^3.9.7"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/cabinet-cli/core",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "cabinet",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "cabinet-cli/core",
    "scripts": {
        "postpack": "rimraf oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "cross-env TS_NODE_PROJECT=\"./test/tsconfig.json\" nyc --extension .ts mocha --require ts-node/register --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md"
    },
    "types": "lib/index.d.ts"
}
